<dt> <%= t(".label", type: type.humanize) %> </dt>
<dd>
    <% if items.blank? %>
      <p><%= t('.empty', type: type.humanize) %></p>
    <% else %>
      <ul class="tabular">
        <% items.each do |item| %>
          <li class='attribute attribute-title'>
            <!-- -->
            <!-- <%= "item.class.name=#{item.class.name}" %> -->
            <!-- <%= "item=#{item}" %> -->
            <!-- -->
            <%#= Updated: hyrax4 # link_to item.title.first, url_for_document(item) %>
            <%= link_to item.title.first, search_state.url_for_document(item) %>
          </li>
        <% end %>
      </ul>
    <% end %>
  </dd>
