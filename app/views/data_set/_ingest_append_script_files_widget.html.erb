<div class="col-12">
  <div class="ingest-append-script-files">
    <div id="ingest_append_script_controls" class="card panel-ingest-append-scripts">
        <button data-toggle="collapse" href="#ingest_append_script_controls_collapse" class="btn">
         <div class="card-header">
          <h2 class="card-title"><%= t('hyrax.base.ingest_append_scripts.actions.header') %>
            <i class="fa fa-chevron-right pull-right"></i></h2>
          </div>
        </button>
      <div id="ingest_append_script_controls_collapse" class="card-body collapse panel-ingest-append-scripts">
        <% if @presenter.ingest_append_script_files.empty? %>
          <%= t('hyrax.base.ingest_append_scripts.empty', type: "Work" ) %>
        <% else %>
          <%= render 'data_set/ingest_append_script_files', presenter: @presenter, all_links: true %>
        <% end %>
      </div>
    </div>
  </div>
</div>
