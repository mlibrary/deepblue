<% if f.object.multiple? key %>
  <%= f.input key, as: :multi_value, input_html: { class: 'form-control', data: { 'key' => key } }, placeholder: "Last name, First name MI", required: f.object.required?(key) %>
<% else %>
  <%= f.input key, input_html: { data: { 'key' => key } }, placeholder: "Last name, First name MI", required: f.object.required?(key) %>
<% end %>